    "version": "0.2.0",
    "configurations": [
        {
            "name": "Generate SVG graph",
            "program": "${workspaceFolder}/utils/svg-graph/index.js",
            "request": "launch",
            "cwd": "${workspaceFolder}/utils/svg-graph",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "Get tweets",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/utils/twitter/main.py",
            "console": "integratedTerminal",
            "python":"${workspaceRoot}/utils/twitter/venv/bin/python",
            "cwd": "${workspaceRoot}/utils/twitter/",
            "justMyCode": true
        }
    ]
}